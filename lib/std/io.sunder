namespace std;
import "sys/sys.sunder";

func print(str: []byte) void {
    if countof(str) == 0 {
        return;
    }
    sys::write(sys::STDOUT_FILENO, &str[0u], countof(str));
}

func println(str: []byte) void {
    print(str);
    print("\n");
}

func eprint(str: []byte) void {
    if countof(str) == 0 {
        return;
    }
    sys::write(sys::STDERR_FILENO, &str[0u], countof(str));
}

func eprintln(str: []byte) void {
    eprint(str);
    eprint("\n");
}
