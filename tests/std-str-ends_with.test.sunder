import "std/core.sunder";

func main() void {
    std::assert(std::str::ends_with("", ""));
    std::assert(std::str::ends_with("a", ""));
    std::assert(not std::str::ends_with("", "a"));

    std::assert(std::str::ends_with("foobar", "r"));
    std::assert(std::str::ends_with("foobar", "ar"));
    std::assert(std::str::ends_with("foobar", "bar"));
    std::assert(not std::str::ends_with("foobar", "bbar"));
    std::assert(not std::str::ends_with("foobar", "foo"));
}
