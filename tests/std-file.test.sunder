import "std/core.sunder";
import "std/fs.sunder";
import "sys/sys.sunder";

func main() void {
    var file: std::file = (:std::file){._fd = (:sys::uint)sys::STDOUT_FILENO};
    var _ = file.write("Hello, file!\n");

    var writer: std::writer = std::writer::init[[std::file]](&file);
    var _ = writer.write("Hello, file_writer!\n");
}
