import "std/core.sunder";

func test[[T]](value: T) void {
    dump value;
    dump -%value;
}

func main() void {
    const x = -%-128s8;
    dump x;
    const y = -%+127s8;
    dump y;

    std::print(std::out(), "\n");

    test[[s8]](s8::MIN);
    std::print(std::out(), "\n");
    test[[s8]](s8::MAX);
    std::print(std::out(), "\n");
    test[[s8]](+1);
    std::print(std::out(), "\n");
    test[[s8]](-1);

    std::print(std::out(), "\n");

    test[[s64]](s64::MAX);
    std::print(std::out(), "\n");
    test[[s64]](s64::MIN);
    std::print(std::out(), "\n");
    test[[s64]](+1);
    std::print(std::out(), "\n");
    test[[s64]](-1);
}
