var v: usize = 0xCAFED00DCAFED00Du;
const w: usize = 0xCAFEBABECAFEBABEu;
const addressof_v: *usize = &v;
const addressof_w: *usize = &w;

const a: [3u]byte = (:[3u]byte)[0xAAy, 0xBBy, 0xCCy];
const addressof_a1: *byte = &a[1u];

const b: [0u]ssize = (:[0u]ssize)[]; # zero-sized object
const addressof_b: *[0u]ssize = &b;

func main() void {
    var x: usize = 0xBADCAFE01DC0FFEEu;
    var addressof_x: *usize = &x;
    var dereference_addressof_x: usize = *addressof_x;

    dump x;
    dump *&x;
    dump *&*&x;
    dump *&*&*&*&x;
    dump dereference_addressof_x;

    var y: [2u][3u]ssize = (:[2u][3u]ssize)[
        (:[3u]ssize)[1s, 3s, 5s],
        (:[3u]ssize)[2s, 4s, 6s]
    ];
    dump y[1u][2u];
    dump *&y[1u][2u];

    const z: usize = 0xDEADBEEFDEADBEEFu;
    var addressof_z: *usize = &z;
    var dereference_addressof_z: usize = *addressof_z;
    dump z;
    dump *&z;
    dump *&*&z;
    dump *&*&*&*&z;
    dump dereference_addressof_z;

    dump v;
    dump *&v;

    dump w;
    dump *&w;

    dump *addressof_v;
    dump *addressof_w;

    dump *addressof_a1;

    dump *addressof_b;
    dump addressof_b;
}
