import "std/page_allocator.sunder";
import "std/vector.sunder";
import "sys";

func main() void {
    var vec: std::vector[[u8]] = std::vector[[u8]]::init(std::allocator::init[[std::page_allocator]](std::page_allocator::the()));
    sys::dump[[usize]](vec.count());
    sys::dump[[usize]](vec.capacity());

    vec.reserve(0);
    sys::dump[[usize]](vec.count());
    sys::dump[[usize]](vec.capacity());

    vec.reserve(1);
    sys::dump[[usize]](vec.count());
    sys::dump[[usize]](vec.capacity());

    vec.reserve(0);
    sys::dump[[usize]](vec.count());
    sys::dump[[usize]](vec.capacity());

    vec.reserve(1);
    sys::dump[[usize]](vec.count());
    sys::dump[[usize]](vec.capacity());

    vec.reserve(2);
    sys::dump[[usize]](vec.count());
    sys::dump[[usize]](vec.capacity());

    vec.reserve(0xBEEF);
    sys::dump[[usize]](vec.count());
    sys::dump[[usize]](vec.capacity());

    vec.reserve(0);
    sys::dump[[usize]](vec.count());
    sys::dump[[usize]](vec.capacity());

    vec.fini();
}
