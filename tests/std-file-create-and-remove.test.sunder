import "std";

func main() void {
    var result = std::file::create("some-file.tmp");
    std::assert(result.is_value());
    var result = std::file::open("some-file.tmp", std::file::OPEN_READ);
    std::assert(result.is_value());
    var result = std::file::remove("some-file.tmp");
    std::assert(result.is_value());
    var result = std::file::open("some-file.tmp", std::file::OPEN_READ);
    std::assert(result.is_error());
}
