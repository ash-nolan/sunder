import "std/core.sunder";
import "std/ascii.sunder";

func main() void {
    std::assert(std::ascii::is_digit('0'));
    std::assert(std::ascii::is_digit('1'));
    std::assert(std::ascii::is_digit('2'));
    std::assert(std::ascii::is_digit('3'));
    std::assert(std::ascii::is_digit('4'));
    std::assert(std::ascii::is_digit('5'));
    std::assert(std::ascii::is_digit('6'));
    std::assert(std::ascii::is_digit('7'));
    std::assert(std::ascii::is_digit('8'));
    std::assert(std::ascii::is_digit('9'));

    std::assert(not std::ascii::is_digit('A'));
    std::assert(not std::ascii::is_digit('a'));
    std::assert(not std::ascii::is_digit('~'));
    std::assert(not std::ascii::is_digit('\n'));
    std::assert(not std::ascii::is_digit('\t'));
    std::assert(not std::ascii::is_digit(' '));
    std::assert(not std::ascii::is_digit('@'));
    std::assert(not std::ascii::is_digit('['));
}
