import "std/core.sunder";
import "std/ascii.sunder";

func main() void {
    dump std::ascii::is_uppercase('A');
    dump std::ascii::is_uppercase('B');
    dump std::ascii::is_uppercase('C');
    dump std::ascii::is_uppercase('Z');

    std::print(std::out(), "\n");

    dump std::ascii::is_uppercase('a');
    dump std::ascii::is_uppercase('b');
    dump std::ascii::is_uppercase('c');
    dump std::ascii::is_uppercase('z');

    std::print(std::out(), "\n");

    dump std::ascii::is_uppercase('1');
    dump std::ascii::is_uppercase('~');
    dump std::ascii::is_uppercase('\n');
    dump std::ascii::is_uppercase('\t');
    dump std::ascii::is_uppercase(' ');
    dump std::ascii::is_uppercase('@');
    dump std::ascii::is_uppercase('[');
}
