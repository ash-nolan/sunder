import "std/core.sunder";
import "std/ascii.sunder";

func main() void {
    std::assert(std::ascii::is_uppercase('A'));
    std::assert(std::ascii::is_uppercase('B'));
    std::assert(std::ascii::is_uppercase('C'));
    std::assert(std::ascii::is_uppercase('Z'));

    std::assert(not std::ascii::is_uppercase('a'));
    std::assert(not std::ascii::is_uppercase('b'));
    std::assert(not std::ascii::is_uppercase('c'));
    std::assert(not std::ascii::is_uppercase('z'));

    std::assert(not std::ascii::is_uppercase('1'));
    std::assert(not std::ascii::is_uppercase('~'));
    std::assert(not std::ascii::is_uppercase('\n'));
    std::assert(not std::ascii::is_uppercase('\t'));
    std::assert(not std::ascii::is_uppercase(' '));
    std::assert(not std::ascii::is_uppercase('@'));
    std::assert(not std::ascii::is_uppercase('['));
}
