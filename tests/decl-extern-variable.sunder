import "std/cstr.sunder";
import "std/io.sunder";
import "sys/sys.sunder"; # Declares sys::argc and sys::argv as extern.

func main() void {
    var bytes: []byte = (:[]byte){*sys::argv, std::cstr::count(*sys::argv)};
    std::out.println(bytes);
}
