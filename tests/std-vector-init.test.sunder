import "std";
import "sys";

func main() void {
    var vec: std::vector[[u32]] = std::vector[[u32]]::init(std::allocator::init[[std::page_allocator]](std::page_allocator::the()));
    sys::dump[[*u32]](vec.start());
    sys::dump[[usize]](vec.count());
    sys::dump[[usize]](vec.capacity());
    sys::dump[[[]u32]](vec.data());
    vec.fini();
}
