func fibonacci(n: usize) usize {
    if n == 0u {
        return 0u;
    }
    if n == 1u {
        return 1u;
    }
    return fibonacci(n-1u) + fibonacci(n-2u);
}

func main() void {
    dump fibonacci(0u);
    dump fibonacci(1u);
    dump fibonacci(2u);
    dump fibonacci(3u);
    dump fibonacci(4u);
    dump fibonacci(5u);
    dump fibonacci(6u);
    dump fibonacci(7u);
    dump fibonacci(8u);
    dump fibonacci(9u);
    dump fibonacci(10u);
}
