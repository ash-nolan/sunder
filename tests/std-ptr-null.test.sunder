import "std/core.sunder";

struct foo {
    var a: u32;
    var b: s32;
}

func main() void {
    dump std::ptr[[u8]]::NULL;
    dump std::ptr[[u16]]::NULL;
    dump std::ptr[[u32]]::NULL;
    dump std::ptr[[u64]]::NULL;
    dump std::ptr[[foo]]::NULL;

    var x: u16 = 0xBEEF;
    var y: u32 = 0xCAFEF00D;

    dump std::ptr[[u16]]::NULL == &x;
    dump std::ptr[[u16]]::NULL == (:*u16)0u;

    dump std::ptr[[u32]]::NULL == &y;
    dump std::ptr[[u32]]::NULL == (:*u32)0u;
}
