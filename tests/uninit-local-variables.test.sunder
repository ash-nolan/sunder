import "sys";

struct foo {
    var w: u8;
    var x: u16;
    var y: u8;
    var z: u64;
}

func main() void {
    var a: u8 = uninit;
    var b: u16 = uninit;
    var c: u32 = uninit;
    var d: u64 = uninit;
    var e: foo = uninit;

    sys::dump[[u8]](a);
    sys::dump[[u16]](b);
    sys::dump[[u32]](c);
    sys::dump[[u64]](d);
    sys::dump[[foo]](e);
}
