import "std/core.sunder";

func main() void {
    dump std::str::contains("", "");
    dump std::str::contains("a", "");
    dump std::str::contains("", "a");

    std::print(std::out(), "\n");

    dump std::str::contains("foobar", "f");
    dump std::str::contains("foobar", "fo");
    dump std::str::contains("foobar", "foo");
    dump std::str::contains("foobar", "fooo");
    dump std::str::contains("foobar", "bar");

    std::print(std::out(), "\n");

    dump std::str::contains("foobar", "foobar");
    dump std::str::contains("foobar", "barfoo");
    dump std::str::contains("foobar", "foobarg");

    std::print(std::out(), "\n");

    dump std::str::contains("kpvwblrizaabmnhz", "ab");
}
