import "std/core.sunder";

func main() void {
    std::assert(std::str::contains("", ""));
    std::assert(std::str::contains("a", ""));
    std::assert(not std::str::contains("", "a"));

    std::assert(std::str::contains("foobar", "f"));
    std::assert(std::str::contains("foobar", "fo"));
    std::assert(std::str::contains("foobar", "foo"));
    std::assert(not std::str::contains("foobar", "fooo"));
    std::assert(std::str::contains("foobar", "bar"));

    std::assert(std::str::contains("foobar", "foobar"));
    std::assert(not std::str::contains("foobar", "barfoo"));
    std::assert(not std::str::contains("foobar", "foobarg"));

    std::assert(std::str::contains("kpvwblrizaabmnhz", "ab"));
}
