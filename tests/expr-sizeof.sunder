const g: usize = sizeof([5u]u8);
const h: usize = sizeof([2u]u64);

const i: u16 = 0xABCDu16;
const j: usize = sizeof(typeof(i));

func main() void {
    dump a;
    dump b;
    dump c;
    dump d;
    dump e;
    dump f;
    dump g;
    dump h;
    dump j;

    dump sizeof(u16);
    dump sizeof(typeof(i));
}
