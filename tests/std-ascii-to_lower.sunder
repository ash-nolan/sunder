import "std/core.sunder";
import "std/ascii.sunder";

func main() void {
    dump std::ascii::to_lower('A') == 'a';
    dump std::ascii::to_lower('B') == 'b';
    dump std::ascii::to_lower('C') == 'c';
    dump std::ascii::to_lower('Z') == 'z';

    dump void::init();

    dump std::ascii::to_lower('a') == 'a';
    dump std::ascii::to_lower('b') == 'b';
    dump std::ascii::to_lower('c') == 'c';
    dump std::ascii::to_lower('z') == 'z';

    dump void::init();

    dump std::ascii::to_lower('~') == '~';
    dump std::ascii::to_lower('\n') == '\n';
    dump std::ascii::to_lower('\t') == '\t';
    dump std::ascii::to_lower(' ') == ' ';
    dump std::ascii::to_lower('@') == '@';
    dump std::ascii::to_lower('[') == '[';
}
