import "std";

func main() void {
    std::assert(std::str::starts_with("", ""));
    std::assert(std::str::starts_with("a", ""));
    std::assert(not std::str::starts_with("", "a"));

    std::assert(std::str::starts_with("foobar", "f"));
    std::assert(std::str::starts_with("foobar", "fo"));
    std::assert(std::str::starts_with("foobar", "foo"));
    std::assert(not std::str::starts_with("foobar", "fooo"));
    std::assert(not std::str::starts_with("foobar", "bar"));
}
