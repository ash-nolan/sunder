import "std/core.sunder";

func main() void {
    dump std::str::starts_with("", "");
    dump std::str::starts_with("a", "");
    dump std::str::starts_with("", "a");

    std::print(std::out(), "\n");

    dump std::str::starts_with("foobar", "f");
    dump std::str::starts_with("foobar", "fo");
    dump std::str::starts_with("foobar", "foo");
    dump std::str::starts_with("foobar", "fooo");
    dump std::str::starts_with("foobar", "bar");
}
