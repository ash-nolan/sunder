import "std/core.sunder";
import "std/ascii.sunder";

func main() void {
    dump std::ascii::is_upper('A');
    dump std::ascii::is_upper('B');
    dump std::ascii::is_upper('C');
    dump std::ascii::is_upper('Z');

    dump void::init();

    dump std::ascii::is_upper('a');
    dump std::ascii::is_upper('b');
    dump std::ascii::is_upper('c');
    dump std::ascii::is_upper('z');

    dump void::init();

    dump std::ascii::is_upper('~');
    dump std::ascii::is_upper('\n');
    dump std::ascii::is_upper('\t');
    dump std::ascii::is_upper(' ');
    dump std::ascii::is_upper('@');
    dump std::ascii::is_upper('[');
}
