import "sys";

struct s {
    var foo: u32;
    var bar: u32;
}

const a: u8 = uninit;
const b: u16 = uninit;
const c: u32 = uninit;
const d: u64 = uninit;
const e: *byte = uninit;
const f: []byte = uninit;
const g: s = uninit;

func main() void {
    const y: u32 = uninit;
    const z: s = uninit;

    sys::dump[[u8]](a);
    sys::dump[[u16]](b);
    sys::dump[[u32]](c);
    sys::dump[[u64]](d);
    sys::dump[[*byte]](e);
    sys::dump[[[]byte]](f);
    sys::dump[[s]](g);

    sys::dump[[u32]](y);
    sys::dump[[s]](z);
}
