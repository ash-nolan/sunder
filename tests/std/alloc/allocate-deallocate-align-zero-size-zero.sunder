import "std/alloc.sunder";
import "std/panic.sunder";

func main() void {
    if (alignof(:void) != 0 or sizeof(:void) != 0) {
        std::panic("unreachable");
    }

    var buf: *void = (:*void)std::allocate(alignof(:void), sizeof(:void));
    std::deallocate((:*byte)buf, alignof(:void), sizeof(:void));
}
