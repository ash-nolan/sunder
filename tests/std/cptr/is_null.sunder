import "std/cptr.sunder";

func main() void {
    var a: u16 = 0xBEEF;
    var b: u32 = 0xCAFEF00D;

    dump std::cptr::is_null[[:u16]](&a);
    dump std::cptr::is_null[[:u16]]((:*u16)0u);

    dump std::cptr::is_null[[:u32]](&b);
    dump std::cptr::is_null[[:u32]]((:*u32)0u);
}
