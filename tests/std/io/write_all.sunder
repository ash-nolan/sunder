import "std/io.sunder";

func main() void {
    var stdout_writer: std::writer = std::writer::init[[:std::stream]](&std::out);
    var stderr_writer: std::writer = std::writer::init[[:std::stream]](&std::err);

    var out_res: std::result[[:usize, :[]byte]] =
        std::write_all(stdout_writer, "Call to write_all (stdout)!\n");
    dump out_res.value();

    var err_res: std::result[[:usize, :[]byte]] =
        std::write_all(stderr_writer, "Call to write_all (stderr)!\n");
    dump err_res.value();
}
