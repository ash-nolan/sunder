import "std/core.sunder";
import "std/ascii.sunder";

func main() void {
    std::assert(std::ascii::is_lowercase('a'));
    std::assert(std::ascii::is_lowercase('b'));
    std::assert(std::ascii::is_lowercase('c'));
    std::assert(std::ascii::is_lowercase('z'));

    std::assert(not std::ascii::is_lowercase('A'));
    std::assert(not std::ascii::is_lowercase('B'));
    std::assert(not std::ascii::is_lowercase('C'));
    std::assert(not std::ascii::is_lowercase('Z'));

    std::assert(not std::ascii::is_lowercase('1'));
    std::assert(not std::ascii::is_lowercase('~'));
    std::assert(not std::ascii::is_lowercase('\n'));
    std::assert(not std::ascii::is_lowercase('\t'));
    std::assert(not std::ascii::is_lowercase(' '));
    std::assert(not std::ascii::is_lowercase('@'));
    std::assert(not std::ascii::is_lowercase('['));
}
