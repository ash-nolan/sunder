import "std/core.sunder";
import "std/page_allocator.sunder";
import "std/string.sunder";

func main() void {
    var string = std::string::init(std::allocator::init[[std::page_allocator]](std::page_allocator::the()));
    dump string.start();
    dump string.count();
    dump string.capacity();
    dump string.data();
    string.fini();
}
