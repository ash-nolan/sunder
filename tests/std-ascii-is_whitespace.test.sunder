import "std";

func main() void {
    std::assert(std::ascii::is_whitespace(' '));
    std::assert(std::ascii::is_whitespace('\t'));
    std::assert(std::ascii::is_whitespace('\n'));
    std::assert(std::ascii::is_whitespace(0x0B));
    std::assert(std::ascii::is_whitespace(0x0C));
    std::assert(std::ascii::is_whitespace(0x0D));

    std::assert(not std::ascii::is_whitespace('A'));
    std::assert(not std::ascii::is_whitespace(0x7F));
    std::assert(not std::ascii::is_whitespace(0xFF));
}
