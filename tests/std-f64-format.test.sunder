# only SUNDER_BACKEND=C
import "std";

func test(f: f64) void {
    std::print_format_line(
        std::out(),
        "{}",
        (:[]std::formatter)[std::formatter::init[[typeof(f)]](&f)]);
}

func main() void {
    test(123.0);
    test(+123.5);
    test(-123.5);
    test(f64::INFINITY);
    test(+f64::INFINITY);
    test(-f64::INFINITY);
    test(f64::NAN);
}
