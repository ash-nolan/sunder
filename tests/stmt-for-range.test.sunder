func seven() usize {
    return 7u;
}

func main() void {
    for n in 3u:7u {
        dump n;
    }

    # Check that shadowing works correctly.
    var i: usize = 0x42u;
    for i in 0xAu:0xDu {
        dump i;
    }
    dump i;

    # Check that the range works with general expressions.
    const begin: usize = 1u + 2u;
    var end: usize = seven();
    for i in begin:end {
        dump i;
    }

    # Check that iteration stops if begin is greater than end.
    for i in 10:9 {
        dump i;
    }

    # Explicit issue using caused by jge instead of jnb during codegen.
    const USIZE_MAX = 0xFFFFFFFFFFFFFFFFu;
    for a in 0:USIZE_MAX {
        dump 0xDEADBEEFu32;
        break;
    }

    # Check for default-to-zero begin.
    for i in 3 {
        dump i;
    }
}
