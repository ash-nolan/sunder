import "std";
import "sys";

func main() void {
    var string = std::string::init(std::allocator::init[[std::page_allocator]](std::page_allocator::the()));
    sys::dump[[*byte]](string.start());
    sys::dump[[usize]](string.count());
    sys::dump[[usize]](string.capacity());
    sys::dump[[[]byte]](string.data());
    string.fini();
}
