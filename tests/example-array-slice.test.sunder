const a: []u16 = (:[]u16)[0xAAAA, 0xBBBB, 0xCCCC];
var b: []u16 = (:[]u16)[0xAAAA, 0xBBBB, 0xCCCC];

func main() void {
    dump countof(a);
    dump a[0];
    dump a[1];
    dump a[2];

    b[2] = 0xDDDD;
    dump countof(b);
    dump b[0];
    dump b[1];
    dump b[2];

    const c: []u16 = (:[]u16)[0x1111, 0x2222, 0x3333];
    dump countof(c);
    dump c[0];
    dump c[1];
    dump c[2];

    var d: []u16 = (:[]u16)[0x1111, 0x2222, 0x3333];
    dump countof(d);
    d[0] = 0xBEEF;
    d[2] = 0xCAFE;
    dump d[0];
    dump d[1];
    dump d[2];

    dump countof((:[]u16)[0xDEAD, 0xBEEF]);
    dump (:[]u16)[0xDEAD, 0xBEEF][1];
}
