import "std";
import "sys";

func main() void {
    std::print_line(std::err(), *sys::error(0));
    std::print_line(std::err(), *sys::error(sys::ENOENT));
    std::print_line(std::err(), *sys::error(sys::EHWPOISON)); # max errno number
    std::print_line(std::err(), *sys::error(sys::EHWPOISON + 1));
    std::print_line(std::err(), *sys::error(ssize::MIN));
}
