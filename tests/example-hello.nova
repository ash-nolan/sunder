func print(str: []byte, count: usize) void {
    if (count == 0u) {
        return;
    }
    const SYS_WRITE: ssize = 1s;
    const STDOUT_FILENO: ssize = 1s;
    syscall(SYS_WRITE, STDOUT_FILENO, &str[0u], count);
}

func main() void {
    print("Hello, world!\n", 14u);
}
