import "std/ptr.sunder";

struct foo {
    var a: u32;
    var b: s32;
}

func main() void {
    dump std::ptr::null[[u8]]();
    dump std::ptr::null[[u16]]();
    dump std::ptr::null[[u32]]();
    dump std::ptr::null[[u64]]();
    dump std::ptr::null[[foo]]();
    dump std::ptr::is_null[[ssize]](std::ptr::null[[ssize]]());
}
