import "sys";

struct s {
    var foo: u32;
    var bar: u32;
}

const a = 0xAAu8;
const b = 0xBBBBu16;
const c = 0xCCCCCCCCu32;
const d = 0xDDDDDDDDDDDDDDDDu64;
const e = (:s){.foo = 0xDEADBEEF, .bar = 0xCAFEF00D};

func main() void {
    const f = (:s32)0xFFFFu16;

    sys::dump[[u8]](a);
    sys::dump[[u16]](b);
    sys::dump[[u32]](c);
    sys::dump[[u64]](d);
    sys::dump[[s]](e);
    sys::dump[[s32]](f);
}
