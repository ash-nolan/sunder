func nl() void {
    # void-returning function that makes dump print a newline.
}

func foo() ssize {
    return +123s;
}

func bar() ssize {
    return -123s;
}

func main() void {
    const t_lt_t: bool = true < true;
    const f_lt_f: bool = false < false;
    const t_lt_f: bool = true < false;
    const f_lt_t: bool = false < true;
    dump t_lt_t;
    dump f_lt_f;
    dump t_lt_f;
    dump f_lt_t;
    dump true < true;
    dump false < false;
    dump true < false;
    dump false < true;
    dump nl();

    const a: ssize = 123s;
    const b: ssize = 456s;
    const c: ssize = a;
    const a_lt_a: bool = a < a;
    const b_lt_b: bool = b < b;
    const a_lt_b: bool = a < b;
    const b_lt_a: bool = b < a;
    const a_lt_c: bool = a < c;
    const b_lt_c: bool = b < c;
    dump a_lt_a;
    dump b_lt_b;
    dump a_lt_b;
    dump b_lt_a;
    dump a_lt_c;
    dump b_lt_c;
    dump nl();

    dump 3u < 3u;
    dump 3u < 5u;
    dump 5u < 3u;
    dump nl();

    var x: ssize = +1s;
    var y: ssize = -1s;
    dump x < y;
    dump y < x;
    dump nl();
}
