# "A" != "AA" => 1
# "A" <  "AA" => 1
# "A" <= "AA" => 1
# "A" >  "AA" => 0
# "A" >= "AA" => 0
# ========
# "AA" == "A" => 0
# "AA" != "A" => 1
# "AA" <  "A" => 0
# "AA" <= "A" => 0
