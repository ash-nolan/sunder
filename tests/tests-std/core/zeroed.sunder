import "std/core.sunder";

struct foo {
    var x: byte;  # 1 byte
                  # 7 bytes padding
    var y: usize; # 8 bytes
    var z: u16;   # 2 bytes
}

func main() void {
    dump std::zeroed[[u8]]();
    dump std::zeroed[[u16]]();
    dump std::zeroed[[u32]]();
    dump std::zeroed[[u64]]();
    dump std::zeroed[[func(usize, ssize) void]]();
    dump std::zeroed[[foo]]();
}
