import "std/page_allocator.sunder";
import "std/vector.sunder";

func main() void {
    var vec: std::vector[[u8]] = std::vector[[u8]]::init(std::page_allocator::the());
    dump vec.count();
    dump vec.capacity();

    vec.reserve(0);
    dump vec.count();
    dump vec.capacity();

    vec.reserve(1);
    dump vec.count();
    dump vec.capacity();

    vec.reserve(0);
    dump vec.count();
    dump vec.capacity();

    vec.reserve(1);
    dump vec.count();
    dump vec.capacity();

    vec.reserve(2);
    dump vec.count();
    dump vec.capacity();

    vec.reserve(0xBEEF);
    dump vec.count();
    dump vec.capacity();

    vec.reserve(0);
    dump vec.count();
    dump vec.capacity();

    vec.fini();
}
