import "std/cptr.sunder";

func main() void {
    dump std::cptr::add[[:u8]]((:*u8)0u, 0);
    dump std::cptr::add[[:u8]]((:*u8)0u, 1);
    dump std::cptr::add[[:u8]]((:*u8)0u, 2);

    dump std::cptr::add[[:u16]]((:*u16)0u, 0);
    dump std::cptr::add[[:u16]]((:*u16)0u, 1);
    dump std::cptr::add[[:u16]]((:*u16)0u, 2);

    dump std::cptr::add[[:u32]]((:*u32)0u, 0);
    dump std::cptr::add[[:u32]]((:*u32)0u, 1);
    dump std::cptr::add[[:u32]]((:*u32)0u, 2);

    dump std::cptr::add[[:u64]]((:*u64)0u, 0);
    dump std::cptr::add[[:u64]]((:*u64)0u, 1);
    dump std::cptr::add[[:u64]]((:*u64)0u, 2);
}
