import "std/core.sunder";
import "std/ascii.sunder";

func main() void {
    std::print_line(std::out(), std::ascii::trimmed_front("foo"));
    std::print_line(std::out(), std::ascii::trimmed_front(" foo"));
    std::print_line(std::out(), std::ascii::trimmed_front("\tfoo"));
    std::print_line(std::out(), std::ascii::trimmed_front(" \t foo"));
    std::print_line(std::out(), std::ascii::trimmed_front(""));
    std::print_line(std::out(), std::ascii::trimmed_front("foo "));
    std::print_line(std::out(), std::ascii::trimmed_front(" foo "));
}
