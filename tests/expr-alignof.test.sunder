import "sys";

const a: usize = alignof(void);
const b: usize = alignof(u8);
const c: usize = alignof(u16);
const d: usize = alignof(u32);
const e: usize = alignof(u64);
const f: usize = alignof(usize);

const g: usize = alignof([5u]u8);
const h: usize = alignof([2u]u64);

const i: u16 = 0xABCDu16;
const j: usize = alignof(typeof(i));

func main() void {
    sys::dump[[usize]](a);
    sys::dump[[usize]](b);
    sys::dump[[usize]](c);
    sys::dump[[usize]](d);
    sys::dump[[usize]](e);
    sys::dump[[usize]](f);
    sys::dump[[usize]](g);
    sys::dump[[usize]](h);
    sys::dump[[usize]](j);

    sys::dump[[usize]](alignof(u16));
    sys::dump[[usize]](alignof(typeof(i)));
}
