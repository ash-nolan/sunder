# Simplified (non-POSIX-compliant) version of `echo`.
import "std";

func main() void {
    var iter = std::argument_iterator::init();
    std::assert(iter.advance()); # Skip the program name.
    var sep = "";
    for iter.advance() {
        std::print(std::out(), sep);
        std::print(std::out(), iter.current());
        sep = " "; # Arguments are separated by " ".
    }
    std::print(std::out(), "\n");
}
