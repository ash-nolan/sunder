# Simplified (non-POSIX-compliant) version of `echo`.
import "std";
import "sys";

func main() void {
    for i in 1:sys::argc {
        if i != 1 {
            std::print(std::out(), " ");
        }
        var arg: *byte = *std::ptr[[*byte]]::add(sys::argv, i);
        std::print(std::out(), std::cstr::as_str(arg));
    }
    std::print(std::out(), "\n");
}
