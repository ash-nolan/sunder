# Simplified (non-POSIX-compliant) version of `echo`.
import "std";
import "sys";

func main() void {
    for i in 1:sys::argc {
        if i != 1 {
            std::print(std::out(), " ");
        }
        var arg: *byte = *std::ptr::add[[*byte]](sys::argv, i);
        std::print(std::out(), std::cstr::to_bytes(arg));
    }
    std::print(std::out(), "\n");
}
