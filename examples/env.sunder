# Simplified (non-POSIX-compliant) version of a zero-argument `env`.
import "std";
import "sys";

func main() void {
    var ptr = sys::envp;
    for not std::ptr[[*byte]]::is_null(ptr) {
        std::print(std::out(), std::cstr::to_str(*ptr));
        std::print(std::out(), "\n");
        ptr = std::ptr[[*byte]]::add(ptr, 1);
    }
    std::print(std::out(), "\n");
}
