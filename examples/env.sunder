# Simplified (non-POSIX-compliant) version of a zero-argument `env`.
import "std";
import "sys";

func main() void {
    var ptr = sys::envp;
    for not std::ptr::is_null[[*byte]](ptr) {
        std::print(std::out(), std::cstr::to_bytes(*ptr));
        std::print(std::out(), "\n");
        ptr = std::ptr::add[[*byte]](ptr, 1);
    }
    std::print(std::out(), "\n");
}
